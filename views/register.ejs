<%- include('header'); -%>
<div class="content">
<form action="/api/register" method="post" >
        <input type = "text" name = "username" id = "username" placeholder="Username" required autofocus>
        <input type="password" id="password" name="password" placeholder="Password" required>
        <input type="text" name="society" id = "society" placeholder="Society" required>
        <input text = "text" name = "contact" id = "contact" placeholder="Contact" required>
        <input type="text" name="phone" id = "phone" placeholder="Phone number" required>
        <input type="text" name="addres" id = "addres" placeholder="Address" required>
        <input type="text" name="city" id = "city" placeholder="City" required>
        <input type="text" name="zipcode" id="zipcode" placeholder="Zip" required>
        <input type = "text" name="fax" id="fax" placeholder="Fax" required>
        <input type="text" name="max_outstanding" id="max_outstanding" placeholder="Max_outstanding">
        <button type="submit">Register</button>
</form>
</div>
<%- include('footer'); -%>

